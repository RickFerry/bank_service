    <bpmn:textAnnotation id="TextAnnotation_10kt0jc">
      <bpmn:text>1. Prepare sms code;
2. Send mobile code;
3. Increase code sending count;</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_0bfvkwe" associationDirection="None" sourceRef="PrepareAndSendVerificationSmsId" targetRef="TextAnnotation_10kt0jc" />
    <bpmn:textAnnotation id="TextAnnotation_0u5ct44">
      <bpmn:text>1. Enter obtained sms code</bpmn:text>
    </bpmn:textAnnotation>
